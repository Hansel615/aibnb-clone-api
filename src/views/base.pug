doctype html
html
  head
    block head
      meta(charset='UTF-8')
      meta(name='viewport' content='width=device-width, initial-scale=1.0')
      link(rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous")
      link(rel='stylesheet' href='/css/style.css')
      link(rel='shortcut icon' type='image/png' href='/img/faviconlogo.png')
      link(rel='stylesheet' href='https://fonts.googleapis.com/css?family=Lato:300,300i,700')
      title Air | #{title}
      link(rel="stylesheet" href="https://pro.fontawesome.com/releases/v5.10.0/css/all.css" integrity="sha384-AYmEC3Yw5cVb3ZcuHtOA93w35dYTsvhLPVnYs9eStHfGJvOvKxVfELGroGkvsg+p" crossorigin="anonymous")
      script(src='https://api.mapbox.com/mapbox-gl-js/v1.11.0/mapbox-gl.js')
      link(href='https://api.mapbox.com/mapbox-gl-js/v1.11.0/mapbox-gl.css' rel='stylesheet')
      script(src="https://js.stripe.com/v3/")
  
  body
    // HEADER
    include _header
        
    // CONTENT
    .base_search

    block content
      .base_video_search
        .base_video_search_b
          form(action='/overview' method='GET')
            .form_box
              label(for='location') LOCATION
              input#location(type='text' name='location' required)
            .form_box
              label(for='startdate') CHECK-IN
              input(type='date' name='startdate' id='fromDate' max='' placeholder='Start Date' required)
            .form_box
              label(for='enddate') CHECK-OUT
              input(type='date' name='enddate' id='toDate' min='' placeholder='End Date' required)
            //- label(for='price[gte]')
            //- input#pricegreat(type='text' name='price[gte]' placeholder='Price Starting from')
            //- label(for='price[lte]')
            //- input#priceless(type='text' name='price[lte]' placeholder='Price Ending ')
            //- label(for='guests')
            //- input#guests(type='text' name='guests' placeholder='Total Guests')
            
            button.btn(type='submit') 
              i.fa.fa-search 
              span Search
        .base_video_search_a
          //- video( autoplay='autoplay' loop='true' controls src="/video/main.mp4" type='video/mp4' width='100%')
          video(width='100%'  autoplay='true' loop muted controls )
            source(src='/video/main.mp4' type='video/mp4')
        
      .base_online_main
        .base_online_main_details
          .base_online_main_details_left
            h2 Belle expériences
            span Donner envie
        .base_online_main_image
          .base_online_main_images_left
            a(href="#")
              img(src="./img/experiences/plant_doctor.jpeg", alt="")
              span  
          .base_online_main_images_right
            a.base_online_main_images_right_a(href="#")
              img.base_online_main_images_right_img(src="./img/experiences/meditate.jpeg", alt="")
              span
                span.base_online_main_images_right_span Profitez
                br
                span.base_online_main_images_right_span Repos 
            a.base_online_main_images_right_a(href="#")
              img.base_online_main_images_right_img(src="./img/experiences/penguin.jpeg", alt="")
              span.base_online_main_images_right_span le papa penguin 
        .base_online_main_background 
          .base_online_main_frontline
            .base_online_main_frontline_left
              h2 Soit un héros
              a(href="#") Get involved
            .base_online_main_frontline_right
              p Allez partout dans le monde

    //- // FOOTER
    include _footer
    
    script(src='https://cdnjs.cloudflare.com/ajax/libs/axios/0.18.0/axios.min.js')
    //- script(src='/js/mapbox.js')
    //- script(src='/js/login.js')
    script(src='/js/signup.js')
    script(src='/js/date.js')
    //- script(type='text/javascript' src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous")
    //- script(type='text/javascript')
      
    script(src='/js/bundle.js')
    script(src='/js/logout.js')
    script(src='/js/host.js')
    script(src='/js/updatepassword.js')
    script(src='/js/updatepersonal.js')
    script(src='/js/mapbox2.js')
    script(src='/js/length.js')
    script(src='/js/index.js')
    script(src='/js/alert.js')
    script(src='/js/forgetPassword.js')
